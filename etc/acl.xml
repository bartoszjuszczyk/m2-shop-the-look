<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Catalog::catalog">
                    <resource id="Magento_Catalog::catalog_inventory">
                        <resource id="Juszczyk_ShopTheLook::looks" title="Juszczyk ShopTheLook - Looks Management" sortOrder="999">
                            <resource id="Juszczyk_ShopTheLook::looks_save" sortOrder="10" title="Juszczyk ShopTheLook - Looks Save"/>
                            <resource id="Juszczyk_ShopTheLook::looks_delete" sortOrder="10" title="Juszczyk ShopTheLook - Looks Delete"/>
                            <resource id="Juszczyk_ShopTheLook::looks_edit" sortOrder="10" title="Juszczyk ShopTheLook - Looks Edit"/>
                            <resource id="Juszczyk_ShopTheLook::looks_create" sortOrder="10" title="Juszczyk ShopTheLook - Looks Create"/>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magento_Catalog::config_catalog">
                                <resource id="Juszczyk_ShopTheLook::config" title="Juszczyk ShopTheLook - Configuration" sortOrder="999"/>
                            </resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
